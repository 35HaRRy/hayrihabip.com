{
  "name": "hayrihabip.com",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run hayrihabip.com:serve-ssr",
    "dev:sls": "ng build -c development && ng run hayrihabip.com:serverless -c development && npm run serve:sls",
    "serve:ssr": "node dist/hayrihabip.com/server/main.js",
    "serve:sls": "serverless offline start --noPrependStageInUrl",
    "build:ssr": "ng build && ng run hayrihabip.com:server",
    "build:sls": "ng build && ng run hayrihabip.com:serverless",
    "prerender": "ng run hayrihabip.com:prerender",
    "deploy": "npm run build:sls && serverless deploy",
    "terminate": "serverless remove"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.2",
    "@angular/common": "~12.1.2",
    "@angular/compiler": "~12.1.2",
    "@angular/core": "~12.1.2",
    "@angular/forms": "~12.1.2",
    "@angular/platform-browser": "~12.1.2",
    "@angular/platform-browser-dynamic": "~12.1.2",
    "@angular/platform-server": "~12.1.2",
    "@angular/router": "~12.1.2",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@nguniversal/common": "^12.1.0",
    "@nguniversal/express-engine": "^12.1.0",
    "angular-in-memory-web-api": "^0.11.0",
    "aws-serverless-express": "^3.4.0",
    "express": "^4.15.2",
    "graceful-fs": "^4.2.8",
    "moment": "^2.29.1",
    "ngx-disqus": "^2.4.3",
    "ngx-highlightjs": "^4.1.4",
    "ngx-tweet": "^2.0.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.3",
    "@angular/cli": "~12.1.3",
    "@angular/compiler-cli": "~12.1.2",
    "@nguniversal/builders": "^12.1.0",
    "@types/express": "^4.17.0",
    "@types/graceful-fs": "^4.1.5",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "serverless": "^2.48.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-offline": "^7.0.0",
    "typescript": "~4.3.2"
  }
